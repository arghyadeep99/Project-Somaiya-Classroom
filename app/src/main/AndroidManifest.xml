<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.somaiya.somaiyaclassroom">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/kjsce"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/kjsce"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".Teacher_Login_Activity" />
        <activity android:name=".Main_Sign_In" />
        <activity android:name=".Forgot_Password_Activity" />
        <activity android:name=".Main_Sign_In_Student" />
        <activity android:name=".Student_Login_Activity" />
        <activity android:name=".courseMaterial" />
        <activity android:name=".Syllabus" />
        <activity android:name=".PreviousPapers" />
        <activity android:name=".Addevent" />
        <activity android:name=".Easysolution" />
        <activity android:name=".syl_act" />
        <activity android:name=".esysol_act" />
        <activity android:name=".lstppr_act" />
        <activity android:name=".course_act"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>